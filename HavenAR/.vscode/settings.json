{
    "dotnet.completion.showCompletionItemsFromUnimportedNamespaces": false,
    "omnisharp.useModernNet": false,
    "omnisharp.path": "latest",
    "omnisharp.enableEditorConfigSupport": false,
    "omnisharp.enableRoslynAnalyzers": false,
    "omnisharp.enableImportCompletion": false,
    "omnisharp.projectLoadTimeout": 30,
    "dotnet.server.useOmnisharp": true,
    "dotnet.unitTests.runSettingsPath": "",
    "dotnet.completion.showNameCompletionSuggestions": false,
    "omnisharp.enableMsBuildLoadProjectsOnDemand": false,
    "omnisharp.enableAsyncCompletion": false,
    "omnisharp.disableMSBuildDiagnosticWarning": true,
    "unity.logLevel": "info",
    "unity.enableRoslynAnalyzers": false,
    "files.exclude": {
        "**/.DS_Store": true,
        "**/.git": true,
        "**/.gitmodules": true,
        "**/*.booproj": true,
        "**/*.pidb": true,
        "**/*.suo": true,
        "**/*.user": true,
        "**/*.userprefs": true,
        "**/*.unityproj": true,
        "**/*.dll": true,
        "**/*.exe": true,
        "**/*.pdf": true,
        "**/*.mid": true,
        "**/*.midi": true,
        "**/*.wav": true,
        "**/*.gif": true,
        "**/*.ico": true,
        "**/*.jpg": true,
        "**/*.jpeg": true,
        "**/*.png": true,
        "**/*.psd": true,
        "**/*.tga": true,
        "**/*.tif": true,
        "**/*.tiff": true,
        "**/*.3ds": true,
        "**/*.3DS": true,
        "**/*.fbx": true,
        "**/*.FBX": true,
        "**/*.lxo": true,
        "**/*.LXO": true,
        "**/*.ma": true,
        "**/*.MA": true,
        "**/*.obj": true,
        "**/*.OBJ": true,
        "**/*.asset": true,
        "**/*.cubemap": true,
        "**/*.flare": true,
        "**/*.mat": true,
        "**/*.meta": true,
        "**/*.prefab": true,
        "**/*.unity": true,
        "**/obj": true,
        "**/bin": true,
        "build/": true,
        "Build/": true,
        "Library/": true,
        "library/": true,
        "obj/": true,
        "Obj/": true,
        "ProjectSettings/": true,
        "temp/": true,
        "Temp/": true,
        "**/Logs": true,
        "**/.vs": true,
        "**/.vscode": false
    },
    "search.exclude": {
        "**/obj": true,
        "**/bin": true,
        "**/Library": true,
        "**/Temp": true,
        "**/*.meta": true
    },
    "files.watcherExclude": {
        "**/obj/**": true,
        "**/bin/**": true,
        "**/Library/**": true,
        "**/Temp/**": true
    },
    "dotnet.defaultSolution": "HavenAR.sln",
    "dotnet.formatting.organizeImportsOnFormat": false
}